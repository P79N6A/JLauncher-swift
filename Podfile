# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'JLauncher' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  pod 'Alamofire', '~> 4.0'
  pod 'Kingfisher', '~> 3.0'
  pod 'SwiftyJSON'
  pod 'MMWormhole', '~> 2.0.0'
end

target 'extension' do
    # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
    use_frameworks!
    
    # Pods for extension
    
    pod 'Alamofire', '~> 4.0'
    pod 'Kingfisher', '~> 3.0'
    pod 'SwiftyJSON'
    pod 'MMWormhole', '~> 2.0.0'
end
